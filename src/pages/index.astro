  <div class="embeds-container">
    <div class="announcements">
      <h1><img src="/src/images/Newspaper.png" />Announcements</h1>
      <a
        class="twitter-timeline"
        href="https://twitter.com/alorubaschksa?ref_src=twsrc%5Etfw"
        data-width="700"
        data-height="500"
      >Tweets by alorubaschksa</a>
      <script async src="https://platform.twitter.com/widgets.js" charset="utf-8"></script>
    </div>
    <div class="calendar">
      <h1><img src="/src/images/Schedule.png" />Upcoming Events</h1>
    <iframe
      src="https://calendar.google.com/calendar/embed?height=600&wkst=1&ctz=Asia%2FKuala_Lumpur&showPrint=0&src=aGFuaW5tb2hhbWVkQGdyYWR1YXRlLnV0bS5teQ&src=YWRkcmVzc2Jvb2sjY29udGFjdHNAZ3JvdXAudi5jYWxlbmRhci5nb29nbGUuY29t&src=ZW4uc2F1ZGlhcmFiaWFuI2hvbGlkYXlAZ3JvdXAudi5jYWxlbmRhci5nb29nbGUuY29t&src=Y19jbGFzc3Jvb204MWIwM2UzMEBncm91cC5jYWxlbmRhci5nb29nbGUuY29t&src=Y19jbGFzc3Jvb202ZGM0Yzg1ZEBncm91cC5jYWxlbmRhci5nb29nbGUuY29t&color=%23039BE5&color=%2333B679&color=%230B8043&color=%23007b83&color=%230047a8&"
      style="border:solid 1px #777"
      width="700"
      height="500"></iframe>
    </div>
  </div>
</BaseLayout>

